{
  "name": "cappi",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.js",
  "homepage": "./",
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "@types/dom-mediacapture-record": "^1.0.2",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.5",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/react-image-crop": "^8.1.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.6.0",
    "electron": "^7.1.2",
    "electron-is-dev": "^1.1.0",
    "electron-reload": "^1.5.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-url-loader": "^1.3.1",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-image-crop": "^8.4.0",
    "react-scripts": "3.2.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "run-p --print-label start:*",
    "start:electron": "NODE_ENV=dev nodemon --watch dist/main.js --exec 'electron .'",
    "start:main": "parcel watch src/main.js --target electron",
    "start:renderer": "parcel --port 3000 src/index.html",
    "build": "run-p --print-label build:*",
    "build:main": "parcel build src/main.js --target electron",
    "build:renderer": "parcel build src/index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "type": "tsc --noEmit --project tsconfig.json",
    "lint": "eslint --format codeFrame src/**.{js,ts,tsx}",
    "eslint-check": "eslint --print-config src/main.js | eslint-config-prettier-check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
